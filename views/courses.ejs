              }
              if(course.type =="senior") {
                reqCur[8] += 1;
              }
            }
            let notGrad = -1
            for(let i = 0; i < reqCur.length; i++) {
              if (reqCur[i] < gradReqs[i]) {
                notGrad = i;
                if(skipFlags.includes(String(notGrad))) {
                  notGrad = -1;
                  continue;
                }
                break;
              }
            }

            noPrereq = [];
            for(let i = 0; i < crsData.length; i++) {
              for(let j = 0; j < crsData[i].length; j++) {
