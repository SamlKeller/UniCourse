            let gpaTotal = 0;
            let predictTotal = 0;
            for (let x = 0; x < 4; x++) {
              for (let m = 0; m < 7; m++) {
                if (crsData[x][m] != 0) {
                  gpaTotal += getWeight(crsData[x][m]);
                  predictTotal += getAverageWeight(crsData[x][m]);
                  classes++;
                  console.log("GPA total: " + gpaTotal);
                }
              }
            }
            if (gpaTotal != 0) {
              document.getElementsByClassName('optimalGPATitle')[0].innerHTML = (gpaTotal/classes).toFixed(2);
              document.getElementsByClassName('optimalGPATitle')[1].innerHTML = (predictTotal/classes).toFixed(2);

            } else {
              document.getElementsByClassName('optimalGPATitle')[0].innerHTML = '--.--';
              document.getElementsByClassName('optimalGPATitle')[1].innerHTML = '--.--';

            }
